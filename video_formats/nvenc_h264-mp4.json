{
    "main_pass":
    [
        "-n", "-c:v", "h264_nvenc",
        "-pix_fmt", ["pix_fmt", ["yuv420p", "p010le"]],
        "-vf", "scale=in_color_matrix=bt709:out_color_matrix=bt709:in_range=full:out_range=tv",
        "-color_range", "tv", "-colorspace", "bt709", "-color_primaries", "bt709", "-color_trc", "bt709"
    ],
    "fake_trc": "bt709",
    "audio_pass": ["-c:a", "aac"],
    "bitrate": ["bitrate","INT", {"default": 50, "min": 1, "max": 999, "step": 1 }],
    "megabit": ["megabit","BOOLEAN", {"default": true}],
    "save_metadata": ["save_metadata", "BOOLEAN", {"default": true}],
    "extension": "mp4"
}
